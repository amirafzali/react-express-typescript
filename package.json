{
  "name": "react-express",
  "version": "1.0.0",
  "description": "react-express boilerpalte",
  "main": "index.ts",
  "scripts": {
    "build": "webpack --mode production && babel src --out-dir out --extensions \".ts,.tsx\" --source-maps inline",
    "start": "npm run build && node out/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon --exec babel-node --extensions \".ts,.tsx\" src/server/index.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Amir Afzali",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/express": "^4.17.1",
    "@types/morgan": "^1.7.37",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.2",
    "css-loader": "^3.2.0",
    "eslint": "^6.2.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^1.19.1",
    "style-loader": "^1.0.0",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-util-is": "^1.0.2",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.11",
    "react-dom": "^16.9.0"
  }
}
